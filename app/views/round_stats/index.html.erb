<h1>Listing round_stats</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Setinfo pass</th>
    <th>Kills</th>
    <th>Deaths</th>
    <th>Hits</th>
    <th>Shots</th>
    <th>Headshots</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @round_stats.each do |round_stat| %>
  <tr>
    <td><%= round_stat.name %></td>
    <td><%= round_stat.setinfo_pass %></td>
    <td><%= round_stat.kills %></td>
    <td><%= round_stat.deaths %></td>
    <td><%= round_stat.hits %></td>
    <td><%= round_stat.shots %></td>
    <td><%= round_stat.headshots %></td>
    <td><%= link_to 'Show', round_stat %></td>
    <td><%= link_to 'Edit', edit_round_stat_path(round_stat) %></td>
    <td><%= link_to 'Destroy', round_stat, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Round stat', new_round_stat_path %>
